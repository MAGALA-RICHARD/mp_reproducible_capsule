            {
              "$type": "Models.Manager, Models",
              "CodeArray": [
                "using APSIM.Numerics;",
                "using Models.Climate;",
                "using System.Linq;",
                "using System;",
                "using Models.Core;",
                "using Models.PMF;",
                "using Models.Soils;",
                "using Models.Utilities;",
                "using APSIM.Shared.Utilities;",
                "using Models.Interfaces;",
                "",
                "namespace Models",
                "{",
                "    [Serializable]",
                "    public class Script : Model",
                "    {",
                "        [Link] private Clock Clock;",
                "        [Link] private Fertiliser Fertiliser;",
                "        [Link] private Summary Summary;",
                "        [Link] private Soil Soil;",
                "        private Accumulator accumulatedRain;",
                "        [Link]",
                "        private ISoilWater waterBalance;",
                "        ",
                "        [Description(\"Crop\")]",
                "        public IPlant Crop { get; set; }",
                "",
                "        [Description(\"Start of sowing window (d-mmm)\")]",
                "        public string StartDate { get; set; }",
                "",
                "        [Description(\"End of sowing window (d-mmm)\")]",
                "        public string EndDate { get; set; }",
                "",
                "        [Description(\"Minimum extractable soil water for sowing (mm)\")]",
                "        public double MinESW { get; set; }",
                "",
                "        [Description(\"Accumulated rainfall required for sowing (mm)\")]",
                "        public double MinRain { get; set; }",
                "",
                "        [Description(\"Duration of rainfall accumulation (d)\")]",
                "        public int RainDays { get; set; }",
                "",
                "        [Display(Type = DisplayType.CultivarName)]",
                "        [Description(\"Cultivar to be sown\")]",
                "        public string CultivarName { get; set; }",
                "",
                "        [Description(\"Sowing depth (mm)\")]",
                "        public double SowingDepth { get; set; }",
                "",
                "        [Description(\"Row spacing (mm)\")]",
                "        public double RowSpacing { get; set; }",
                "",
                "        [Description(\"Plant population (/m2)\")]",
                "        public double Population { get; set; }",
                "        ",
                "        ",
                "        [EventSubscribe(\"StartOfSimulation\")]",
                "        private void OnSimulationCommencing(object sender, EventArgs e)",
                "        {",
                "            accumulatedRain = new Accumulator(this, \"[Weather].Rain\", RainDays);",
                "        }",
                "",
                "        [EventSubscribe(\"DoManagement\")]",
                "        private void OnDoManagement(object sender, EventArgs e)",
                "        {",
                "            accumulatedRain.Update();",
                "            ",
                "            if (DateUtilities.WithinDates(StartDate, Clock.Today, EndDate) &&",
                "                !Crop.IsAlive &&",
                "                MathUtilities.Sum(waterBalance.ESW) > MinESW &&",
                "                accumulatedRain.Sum > MinRain)",
                "            {",
                "                Crop.Sow(population: Population, cultivar: CultivarName, depth: SowingDepth, rowSpacing: RowSpacing);    ",
                "            }",
                "        }",
                "    }",
                "}"
              ],
              "Parameters": [
                {
                  "Key": "Crop",
                  "Value": "[Maize]"
                },
                {
                  "Key": "StartDate",
                  "Value": "18-apr"
                },
                {
                  "Key": "EndDate",
                  "Value": "1-may"
                },
                {
                  "Key": "MinESW",
                  "Value": "100"
                },
                {
                  "Key": "MinRain",
                  "Value": "5"
                },
                {
                  "Key": "RainDays",
                  "Value": "7"
                },
                {
                  "Key": "CultivarName",
                  "Value": "B_110"
                },
                {
                  "Key": "SowingDepth",
                  "Value": "25"
                },
                {
                  "Key": "RowSpacing",
                  "Value": "750"
                },
                {
                  "Key": "Population",
                  "Value": "8.65"
                }
              ],
              "Name": "Sow using a variable rule",
              "ResourceName": null,
              "Children": [],
              "Enabled": true,
              "ReadOnly": false
            },
            {
              "$type": "Models.Report, Models",
              "VariableNames": [
                "[Clock].Today",
                "[Simulation].Name as Plotid",
                "[Clock].Today.Year as year",
                "sum([Nutrient].FOM.C)  from 01-jan to [Clock].Today as FOM.c ",
                "sum([Nutrient].TotalOrganicN[1])  from 01-jan to [Clock].Today as TON",
                "sum([Soil].Nutrient.TotalC)/1000 from 01-jan to [clock].Today as soc",
                "[SurfaceOrganicMatter].C as Total_surfaceCArbon",
                "[SurfaceOrganicMatter].wt as surface_organic_matter",
                "sum([Nutrient].Catm) as Soil_Respiration",
                "sum([Soil].Nutrient.TotalC[3:5])/1000 as Deeplayer_soil_Carbon",
                "sum([Soil].Nutrient.TotalC[1:3])/1000 as Toplayer_soil_Carbon",
                "sum of sum([Nutrient].N2Oatm[1:3]) from [carbon].DayAfterLastOutput to [Clock].Today as n20",
                "sum of sum([Nutrient].N2Oatm[1:6] from 01-jan to [Clock].Today as N20D20cm",
                "sum of sum([Nutrient].N2Oatm[1:9]) from 01-jan to [Clock].Today as N20",
                "[Soil].Nutrient.TotalC[1]/1000 from [carbon].DayAfterLastOutput to [clock].Today as SOC_0_15CM // soil carbon in the first layer",
                "[Soil].Nutrient.TotalC[2]/1000 from [clock].StartOfSimulation to [clock].Today as SOC_1_2",
                "SOC_1_2 + SOC_0_15CM as soccx",
                "sum([Soil].Nutrient.TotalC[:1])/1000 as SOCD40CM",
                "sum of [Nutrient].Catm[1] from 1-jan to today as RespD20cm",
                "sum of [Nutrient].Catm[1] from 1-jan to today as RespD40cm",
                "[Soil].Water.Volumetric as DUL",
                "[Soil].SoilWater.PAW as PAW",
                "[Report].Name as Plotid",
                "sum of ([Nutrient].Catm[1]) from 01-jan to [Clock].Today as Top_respiration",
                "sum of sum([Nutrient].Catm[1:9]) from 01-jan to [Clock].Today as whole_respiration",
                "[Maize].Root.Total.Wt * 10 as RootBiomass",
                "[Soil].Nutrient.TotalC[7] as deepC",
                "[Soil].Nutrient.TotalC[8] as deepC8",
                "[Soil].Nutrient.TotalC[9] as deepC9",
                "[Soil].Nutrient.TotalC[10] as deepC10",
                "mean of [Maize].LAI  from [carbon].DayAfterLastOutput to [Clock].Today as lai",
                "[Soil].Nutrient.TotalC[1]/1000 as carbo",
                "[Soil].Temperature.Value[1] as tmp1 ",
                "[Soil].Temperature.Value[2] as tmp2 ",
                "[Soil].Temperature.Value[1] + [Soil].Temperature.Value[2] as tmp ",
                "",
