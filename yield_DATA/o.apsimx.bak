            {
              "$type": "Models.Manager, Models",
              "CodeArray": [
                "using Models;",
                "using System;",
                "using System.Collections.Generic;",
                "using System.Globalization;",
                "using System.Linq;",
                "using APSIM.Shared.Utilities;",
                "using Models.Climate;",
                "using Models.Core;",
                "using Models.PMF;",
                "",
                "namespace Models",
                "{",
                "    [Serializable]",
                "    public class Script : Model",
                "    {",
                "        [Link] private Clock Clock = null;",
                "        [Link] private Fertiliser Fertiliser = null;",
                "        [Link] private ISummary Summary = null;",
                "        [Link] private Weather Weather = null;",
                "",
                "        [Separator(\"Crop in the rotation to apply fertilizers\")]",
                "        [Description(\"Crop\")]",
                "        public IPlant Crop { get; set; }",
                "",
                "        [Separator(\"Fertilizer application details\")]",
                "        [Description(\"Type of fertiliser to apply? Select from a list.\")]",
                "        [Display(Type = DisplayType.FertiliserType)]public string FertiliserType { get; set; }",
                "",
                "        [Description(\"Depth to apply the fertilizer.\")]",
                "        public double Depth2Apply { get; set; }",
                "",
                "        [Description(\"Fraction to apply at sowing (0-1). If it is a single application, set to 1.\")]",
                "        public double FractionToApplyFirst { get; set; }",
                "",
                "        [Description(\"Enter the total amount of fertiliser to be applied (kg/ha).\")]",
                "        public double Amount { get; set; }",
                "",
                "        [Description(\"Enter other potential dates as dd-MMM with comma separation (any year entered will be ignored).\")]",
                "        public string[] FertiliserDates { get; set; }",
                "",
                "        private bool hasAppliedBalance = false;",
                "        private DateTime sowingDate;",
                "        private List<DateTime> candidateDatesThisSeason = new List<DateTime>();",
                "",
                "        [EventSubscribe(\"Sowing\")]",
                "        private void OnSowing(object sender, EventArgs e)",
                "        {",
                "            var cropModel = sender as Model;",
                "",
                "            // Only act for the selected crop in rotation",
                "            if (Crop != null && cropModel != null &&",
                "                cropModel.Name.Equals((Crop as IModel)?.Name, StringComparison.OrdinalIgnoreCase))",
                "            {",
                "                // Apply first fraction at sowing (if any)",
                "                var firstN = FractionToApplyFirst * Amount;",
                "                if (firstN > 0)",
                "                    Fertiliser.Apply(amount: firstN, type: FertiliserType, depth: Depth2Apply);",
                "",
                "                // Reset per-season state",
                "                hasAppliedBalance = false;",
                "                sowingDate = Clock.Today;",
                "                candidateDatesThisSeason = BuildSeasonDates(FertiliserDates, sowingDate.Year)",
                "                    // Keep only dates on/after sowing to avoid firing before crop exists",
                "                    .Where(d => d >= sowingDate)",
                "                    .OrderBy(d => d)",
                "                    .ToList();",
                "            }",
                "        }",
                "",
                "        [EventSubscribe(\"DoManagement\")]",
                "        private void OnDoManagement(object sender, EventArgs e)",
                "        {",
                "            // Already finished balance this season",
                "            if (hasAppliedBalance)",
                "                return;",
                "",
                "            // No crop or crop not alive",
                "            if (Crop == null || !Crop.IsAlive)",
                "                return;",
                "",
                "            // No candidate dates supplied",
                "            if (FertiliserDates == null || FertiliserDates.Length == 0)",
                "                return;",
                "",
                "            // Remaining N (balance) after the sowing fraction",
                "            var firstN = FractionToApplyFirst * Amount;",
                "            var remainingN = Amount - firstN;",
                "",
                "            if (remainingN <= 0)",
                "            {",
                "                hasAppliedBalance = true; // nothing left to apply",
                "                return;",
                "            }",
                "",
                "            // Try to apply on any candidate that matches TODAY and has no rain",
                "            foreach (var d in candidateDatesThisSeason)",
                "            {",
                "                // Use month/day equality (DateUtilities ignores year when given \"dd-MMM\")",
                "                if (Clock.Today.Date == d.Date && Weather.Rain >=0)",
                "                {",
                "                    Fertiliser.Apply(amount: remainingN, type: FertiliserType, depth: Depth2Apply);",
                "",
                "                    Summary.WriteMessage(this,",
                "                        $\"Applied remaining {remainingN:0.##} kg N as {FertiliserType} at {Depth2Apply} mm on {Clock.Today:dd-MMM-yyyy}.\",",
                "                        MessageType.Information);",
                "",
                "                    Summary.WriteMessage(this, $\"Rain was: {Weather.Rain:0.##} mm.\", MessageType.Information);",
                "",
                "                    hasAppliedBalance = true;",
                "                    return;",
                "                }",
                "            }",
                "",
                "            // If we've passed the last candidate date and still haven't applied, throw an error ONCE.",
                "            if (candidateDatesThisSeason.Count > 0)",
                "            {",
                "                var lastCandidate = candidateDatesThisSeason[^1];",
                "                if (Clock.Today.Date > lastCandidate.Date && !hasAppliedBalance && remainingN > 0)",
                "                {",
                "                    var listStr = string.Join(\", \", candidateDatesThisSeason.Select(d => d.ToString(\"dd-MMM\")));",
                "                    var msg =",
                "                        $\"No rain-free candidate date was found among [{listStr}] for crop '{Crop.Name}'. \" +",
                "                        $\"Remaining nitrogen ({remainingN:0.##} kg/ha) was not applied.\";",
                "",
                "                    // Log as error and stop the simulation so the issue is visible in batch runs",
                "                    Summary.WriteMessage(this, msg, MessageType.Error);",
                "                    throw new Exception(msg);",
                "                }",
                "            }",
                "        }",
                "",
                "        /// <summary>",
                "        /// Parse user-supplied \"dd-MMM\" (or \"dd-MMM-yyyy\") strings into DateTimes in the given year.",
                "        /// Any year provided in input is ignored; we force the supplied <paramref name=\"year\"/>.",
                "        /// </summary>",
                "        private static IEnumerable<DateTime> BuildSeasonDates(IEnumerable<string> inputs, int year)",
                "        {",
                "            if (inputs == null)",
                "                yield break;",
                "",
                "            foreach (var raw in inputs)",
                "            {",
                "                if (string.IsNullOrWhiteSpace(raw))",
                "                    continue;",
                "",
                "                var s = raw.Trim();",
                "",
                "                // Try parsing day-month (ignore any year), then replace year.",
                "                // Accept both \"dd-MMM\" and \"dd-MMM-yyyy\".",
                "                if (DateTime.TryParseExact(s, new[] { \"dd-MMM\", \"d-MMM\", \"dd-MMM-yyyy\", \"d-MMM-yyyy\" },",
                "                                           CultureInfo.InvariantCulture, DateTimeStyles.None, out var dt))",
                "                {",
                "                    // Force to the requested season year (ignore any year that might have been in the string)",
                "                    yield return new DateTime(year, dt.Month, dt.Day);",
                "                }",
                "                else",
                "                {",
                "                    // If format is wrong, fail fast so users can fix the input.",
                "                    throw new FormatException(",
                "                        $\"Invalid fertiliser date '{s}'. Use 'dd-MMM' (e.g., 15-May) or 'dd-MMM-yyyy'.\");",
                "                }",
                "            }",
                "        }",
                "    }",
                "}",
