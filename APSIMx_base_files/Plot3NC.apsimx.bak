                    "             ",
                    "              ",
                    "           var Crop = existingCrop;",
                    "            ",
                    "            Model crop = sender as Model;",
                    "            if (Crop != null && crop.Name.ToLower() == (Crop as IModel).Name.ToLower())",
                    "            {",
                    "            agb1  = Crop.AboveGround.Wt * 10;",
                    "             Summary.WriteMessage(this, $\"Above ground biomass is: {agb1}.\", MessageType.Information);",
