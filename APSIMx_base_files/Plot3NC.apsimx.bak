                    "          {",
                    "          var fraction = 0.0;",
                    "          SOM.Incorporate(fraction, Depth);",
                    "          }",
                    "          else if (TillageDate.Date == clock.Today.Date && Crop != null && year >1968)",
                    "          {",
                    "          var fraction = 0.5;",
                    "          SOM.Incorporate(fraction, Depth);",
                    "          }",
                    "          }",
