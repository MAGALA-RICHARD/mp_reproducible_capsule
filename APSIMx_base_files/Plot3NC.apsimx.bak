                    "            {",
                    "                string key = keyValuePairs[0];",
                    "                int value = int.Parse(keyValuePairs[1]);",
                    "                myDictionary.Add(key, value);",
                    "            }",
                    "           } ",
                    "          ",
                    "           if (TillageDate.Date == clock.Today.Date && Crop != null){",
