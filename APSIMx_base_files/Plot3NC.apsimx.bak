                    "    int EvaluationDate = 1998;",
                    "    int year = TillageDate.Year; ",
                    "    int Depth;",
                    "    if (year >= EvaluationDate){",
                    "      Depth = 400;",
                    "    }else{",
                    "      Depth = 150;",
                    "    } ",
                    "     ",
                    "    Dictionary<string, int> myDictionary = new Dictionary<string, int>();",
                    "    var Crop = existingCrop;",
                    "    foreach (string stringentry in Years)",
                    "           {",
                    "           char[] delimiter = { '=' };",
                    "           string[] keyValuePairs = stringentry.Split(delimiter, StringSplitOptions.RemoveEmptyEntries);",
